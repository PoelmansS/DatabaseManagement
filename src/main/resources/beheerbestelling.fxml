        <TextField fx:id="addID" layoutX="91.0" layoutY="360.0" prefHeight="25.0" prefWidth="67.0" />
        <TextField fx:id="addKlant" layoutX="167.0" layoutY="360.0" prefHeight="25.0" prefWidth="92.0" />
        <TextField fx:id="addComputer" layoutX="267.0" layoutY="360.0" prefHeight="25.0" prefWidth="85.0" />
        <TextField fx:id="addPrice" layoutX="362.0" layoutY="360.0" prefHeight="25.0" prefWidth="78.0" />

        <Text layoutX="91.0" layoutY="349.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID" wrappingWidth="53.0" />
        <Text layoutX="167.0" layoutY="349.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Klant" wrappingWidth="92.0" />
        <Text layoutX="267.0" layoutY="348.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Computer" />
        <Text layoutX="362.0" layoutY="347.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price" />

