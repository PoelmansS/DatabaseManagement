                <TableColumn fx:id="klantColumn" prefWidth="75.0" text="Type" />
                <TableColumn fx:id="computerColumn" prefWidth="75.0" text="Price" />
                <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Size" />
            </columns>
        </TableView>
        <Text layoutX="9.0" layoutY="28.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Beheer Bestellingen" wrappingWidth="567.21875">
            <font>
