<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.kuleuven.pccustomizer.controller.config.configCpu">
   <children>
       <ImageView fitHeight="421.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
           <image>
               <Image url="@../../../build/resources/main/_c_bg.png" />
           </image>
       </ImageView>
      <Button fx:id="btnAdd" layoutX="465.0" layoutY="349.0" mnemonicParsing="false" text="add component" />
      <Button fx:id="btnClose" layoutX="31.0" layoutY="349.0" mnemonicParsing="false" text="end config" />
      <TableView fx:id="tableView" layoutX="212.0" layoutY="58.0" prefHeight="266.0" prefWidth="356.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="100.0" text="Name" />
            <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Price" />
            <TableColumn fx:id="threadsColumn" prefWidth="75.0" text="Threads" />
            <TableColumn fx:id="coresColumn" prefWidth="75.0" text="Cores" />
            <TableColumn fx:id="clockSpeedColumn" prefWidth="100" text="Clock Speed" />
            <TableColumn fx:id="powerUsageColumn" prefWidth="100" text="PowerUsage" />
        </columns>
      </TableView>
       <Label layoutX="24.0" layoutY="7.0" prefHeight="30.0" prefWidth="101.0" text="CPU">
           <font>
               <Font name="System Bold Italic" size="20.0" />
           </font></Label>
       <TableView fx:id="componentView" layoutX="34.0" layoutY="58.0" prefHeight="266.0" prefWidth="140.0">
           <columns>
               <TableColumn fx:id="componentCollum" prefWidth="141.0" text="componenten" />
           </columns>
       </TableView>
   </children>
</AnchorPane>
